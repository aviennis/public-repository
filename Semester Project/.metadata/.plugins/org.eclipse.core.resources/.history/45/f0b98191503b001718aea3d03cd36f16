	{
		return nameOfIndex;
	}
	public boolean[] getUnique()
	{
		return isUnique;
	}
	public boolean[] getIndex()
	{
		return isIndex;
	}
	public boolean[] getNotNull()
	{
		return notNull;
	}
	public boolean[] getPrimaryKey()
	{
		return primaryKey;
	}
	public ArrayList<BTreeImplementation> getTrees()
	{
		return BTrees;
	}
	
	
}
